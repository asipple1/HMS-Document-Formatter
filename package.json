{
  "name": "hms-doc-formatter",
  "version": "1.0.0",
  "description": "Honolulu Med Spa document formatter tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ImageShrink\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asipple1/HMS-Document-Formatter.git"
  },
  "keywords": [
    "node.js",
    "electron"
  ],
  "author": "Andy Sipple",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.3.1",
    "electron-packager": "^15.0.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "slash": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/asipple1/HMS-Document-Formatter/issues"
  },
  "homepage": "https://github.com/asipple1/HMS-Document-Formatter#readme"
}
